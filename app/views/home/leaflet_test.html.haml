- content_for :javascripts do
  = javascript_include_tag "leaflet"
  = javascript_include_tag "leaflet_lib"

  :javascript
    var geojsonFeature = {
        "type": "Feature",
        "properties": {
            "name": "#{@community_area.name}",
            "density": "100"
        },
        "geometry": #{@community_area.geometry}
    };

    LeafletLib.initialize(geojsonFeature, #{@community_area.centroid_as_lat_lng}, 14);

#map